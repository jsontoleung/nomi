@import "weui.wxss";
.page{
  width:100%;
}
view{
  font-family: "方正";
}
text{
  font-family: "方正";
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.ul {
  margin: 0;
  padding: 0;
  border: 0;
  list-style: none;
}
.absolute{
  position: absolute;
  top: 0px;
  left: 0px;
  bottom: 0px;
  right: 0px;
  margin: auto;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.tl{
  text-align: left;
}
.tr{
  text-align: right;
}
.tc{
  text-align: center;
}
.cl{
  clear: both;
  height: 0px;
  overflow: hidden;
}
image{
  border: none;
  vertical-align: middle;
}
.max-img{
  max-height: 100% !important;
  max-width: 100% !important;
}
.ylw-img{
  height: auto;
  max-width: 100%;
}
.img{
  width: 100% !important;
  height: 100% !important;
  display: block;
}
.wxParse-img {
    width:100% !important;
}

.footer {
  display: block;
}
.footer {
    z-index: 9999!important;
}
.footer {
  width: 100%;
  height:100rpx;
  max-width: 640px;
  position: fixed;
  bottom: 0;
  left: 50%;
  -webkit-transform: translate3d(-50%,0,0);
  transform: translate3d(-50%,0,0);
  overflow: hidden;
  background-color: #fff;
  border-top: #eceded;
  box-shadow: 0 0 4px rgba(0,0,0,.2);
}

.footer .li {
  float: left;
  width: 33.333%;
  text-align: center;
  margin: 5px 0px;
  height: 100%;
}

.footer .a,
.footer .a-focus,
.footer .a-hover {
    color: #777;
    font-size: 26rpx;
}

/* .footer .a.active {
     color: #378E41; 
} */

.footer .nav-controller {
    position: relative;
    height: 100%;
    display: block;
}

.footer .nav-controller .fb-name {
  text-align: center;
  font-size: 24rpx;
  color:#666666;
  padding-bottom: 14rpx;
}

.footer .nav-controller .fb-names {
  text-align: center;
  font-size: 24rpx;
  color:#f6afc3;
  padding-bottom: 14rpx;
}


.fb-home image {
    background-repeat: no-repeat;
    background-size: 20px auto;
    width:50rpx;
    height: 50rpx;
    margin: auto;
}

.fb-group image{
    background-repeat: no-repeat;
    background-size: 20px auto;
    margin: auto;
    width: 50rpx;
    height: 50rpx;
}

.fb-list {
    background-repeat: no-repeat;
    background-size: 20px auto;
    height: 12px;
    margin: auto;
    width: 20px;
}

.fb-user image{
    background-repeat: no-repeat;
    background-size: 20px auto;
    margin: auto;
    width: 50rpx;
    height: 50rpx;
}
