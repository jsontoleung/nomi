/* pages/product/detail/index.wxss */
.page{
    width:100%;
    background: #f2f2f2;
    padding-bottom: 60px;
    position: absolute;
}
button::after{
   content: none;
}

swiper {
    width:100%;
    height:460rpx;
}

swiper swiper-item image {
    width:100%;
    height:100%;
}

/* 会员优惠 */
.btn-vip {
    width:100%;
    height:70rpx;
    background: #fff;
    margin-top: 20rpx;
}
.btn-vip .vip-name {
    float: left;
    font-size: 30rpx;
    text-indent: 30rpx;
    margin-top: 2%;
    color:#282828;
    font-weight: bold;
}
.btn-vip .vip-buy {
    float: right;
    font-size: 28rpx;
    margin-right: 30rpx;
    margin-top: 1.8%;
    color:#fff;
    background-color: #282828;
    padding:0rpx 12rpx;
    border-radius: 10rpx;
}
.btn-vip .vip-money {
    float: right;
    font-size: 30rpx;
    margin-right: 30rpx;
    margin-top: 2%;
    font-weight: bold;
}

/* 标题 */
.btn-title {
    width:100%;
    height:135rpx;
    background-color: #e8396a;
    color:#fff;
}
.btn-title .title-left {
    width:100%;
    float: left;
    font-size: 30rpx;
    margin-top: 2%;
    text-indent: 30rpx;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.btn-title .title-right {
    float: right;
    font-size: 34rpx;
    margin-right: 30rpx;
    font-weight: bold;
    margin-top: 2%;
}
.btn-title .title-num {
    float:left;
    font-size: 32rpx;
    color:#fff;
    text-indent: 30rpx;
    margin-top: 2%;
}


/* 已选择数量 */
.btn-num {
    width:100%;
    height:100rpx;
    background: #fff;
    margin-top: 20rpx;
    position:relative;
}
.btn-num .num-left {
    float: left;
    font-size: 28rpx;
    margin-top: 3%;
    text-indent: 30rpx;
}
.btn-num .num-right {
    float: right;
    margin-right: 30rpx;
    margin-top: 3%;
    width: 180rpx;
}
.btn-num .num-right .stepper {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.btn-num .num-right .stepper text {
    background-color: #fff;
    border: 1px solid #eee;
    width: 26px;
    height: 26px;
    line-height: 26px;
    text-align: center;
    font-weight: 900;
    color: #666;
}
.btn-num .num-right .stepper input {
    width: 30px;
    height: 25px;
    text-align: center;
    border-top:  1px solid #eee;
    border-bottom:  1px solid #eee;
}


/* 送货 */
.btn-song {
    width:100%;
    padding:1% 0;
    margin-top: 20rpx;
    background: #fff;
    position: relative;
}

.btn-song .sont-bottom {
    width:90%;
    height:135rpx;
    /* position:absolute;
    bottom:27rpx;
    left:66rpx; */
    margin-left: 5%;
}
.btn-song .sont-bottom .bottom-con {
    float:left;
    padding-left: 60rpx;
}
/* .btn-song .sont-bottom .bottom-con:not(:first-child) {
  padding-left: 60rpx;
} */
.btn-song .sont-bottom .bottom-con image {
    width:28rpx;
    height:28rpx;
}
.btn-song .sont-bottom .bottom-con text {
    font-size: 28rpx;
    margin-left: 6rpx;
    overflow: hidden;
}


/* 选择门店 */
.radio-group {
    width:100%;
    padding: 4% 0;
    margin-top: 20rpx;
    background-color: #fff;
    display: inline-block;
}
.radio {
    width: 50%;
    height:60rpx;
    float: left;
    text-align: center;
    font-size: 30rpx;
    color:#23c40d;
}


/* 服务次数 */
.number-btn {
    width:100%;
    padding: 4% 0;
    margin-top: 20rpx;
    background-color: #fff;
    display: inline-block;
}
.number-btn .serve-name {
    font-size: 30rpx;
    float:left;
    margin-left: 5%;
}
.number-btn .serve-num {
    font-size: 30rpx;
    float: right;
    margin-right: 5%;
    color:#e8396a;
}



/* 评论 */
.btn-com {
    width:100%;
    height:250rpx;
    background-color: #fff;
    margin-top: 20rpx;
    position:relative;
}
.btn-com .com-top {
    width:92%;
    padding:30rpx 4%;
}
.btn-com .com-top text {
    font-size: 30rpx;
    float:left;
}
.btn-com .com-top image {
    float:right;
    width:22rpx;
    height:28rpx;
    margin-top: 4rpx;
}
.btn-com .com-name {
    width:92%;
    margin: 30rpx auto;
}
.btn-com .com-name image {
    width:50rpx;
    height:50rpx;
    border-radius: 100%;
    float:left;
}
.btn-com .com-name text {
    font-size: 26rpx;
    color:#888;
    float:left;
    margin-left: 10rpx;
    margin-top: 6rpx;
}
.btn-com .com-title {
    width:92%;
    margin-left: 4%;
    line-height: 67rpx;
    font-size: 24rpx;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.btn-com .com-time {
    width:92%;
    margin: 0rpx auto 0;
    font-size: 24rpx;
    color:#888;
}


/* 产品内容 */
.desc-btn {
    width:100%;
    height:80rpx;
    margin-top: 20rpx;
    border-bottom: 1px solid #efefef;
    background-color: #fff;
}
.desc-pro {
    font-size: 30rpx;
    text-indent: 30rpx;
    line-height: 84rpx;
}
.desc-con {
    width:100%;
    background-color: #fff;
}



/* 其他 */
.btn-else {
    width:100%;
    height:350rpx;
    margin: 30rpx 0rpx 40rpx;
    background-color: #fff;
}
.btn-else .else-top {
    width:92%;
    height:40rpx;
    font-size: 30rpx;
    padding:35rpx 4% 15rpx;
}
.pic {
    width:44%;
    height:210rpx;
    float:left;
    margin:20rpx 20rpx;
    position: relative;
}
.pic .pic-bottom {
    width:100%;
    height:46rpx;
