    padding-bottom: 60rpx;
    position: fixed;
    top:31%;
    margin-left: 5%;
    background-color: #000;
    opacity: 0.5;
    border-radius: 14rpx;
}
.btn-popup .user-call {
    font-size: 32rpx;
    color:red;
    text-align: center;
    line-height: 70rpx;
}
.btn-popup .deletes {
    width:70rpx;
    height:70rpx;
    position:absolute;
    top:-6rpx;
    right:0rpx;
}
.btn-popup .popup-box {
    width:90%;
    margin:30rpx 5%;
}
.btn-popup .hover-bg {
    background-color: #7A67EE;
