}
/* 底部弹出窗 */
.modals{
    width: 100%;
    height: 500rpx;
    position: fixed;
    bottom: 0;
    left: 50%;
    -webkit-transform: translate3d(-50%,0,0);
    transform: translate3d(-50%,0,0);
    overflow: hidden;
    background-color: #fff;
    border-top: #eceded;
    box-shadow: 0 0 4px rgba(0,0,0,.2);
    z-index: 9999;
}
.modals-cancel {
    width: 100%;
    height: 100vh;
    position: fixed;
    bottom: 0;
    left: 50%;
    -webkit-transform: translate3d(-50%,0,0);
    transform: translate3d(-50%,0,0);
    overflow: hidden;
    background-color: rgba(0, 0, 0, 0.5);
    border-top: #eceded;
    box-shadow: 0 0 4px rgba(0,0,0,.2);
    z-index: 9998;
}
 
.bottom-dialog-body {
