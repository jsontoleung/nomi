    opacity: 0.9;
    
}
/*将canvas移到看不到的地方生成*/
canvas{
  position: absolute;
  left: 0;
  right: 0;
  top: -9999rpx;
  bottom: 0;
  margin: auto;
}
.preview{
  position:fixed;
  top:0;
  left:0;
  width:100vw;
  height:100vh;
  background-color:rgba(0,0,0,0.8);
  z-index: 2;
  text-align: center;
  display:flex; 
  flex-flow:column;
}
 
.preview image{
  width: 80%;
  height: 80%;
  margin: 10%;
  margin-bottom: 5%;
}
 
.preview button{
  margin:0 20rpx;
  border-radius:50px;
  background-color:#ee2a65;
  width:60%;
  padding: 10rpx 0;
  color: rgba(255,255,255,.8)
}
/* 分享朋友和朋友圈弹出 */
.shareall{
