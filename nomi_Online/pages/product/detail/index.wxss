}
.btn-popup .popup-box .user-name {
    font-size: 26rpx;
    text-align: center;
    color:#fff;
}
.btn-popup .popup-box .user-money {
    font-size: 26rpx;
    text-align: center;
    color:#fff;
    line-height: 70rpx;
}
.btn-popup .popup-box .user-title {
    font-size: 24rpx;
    text-align: center;
    color:#fff;
}


/* 返回首页--start */
.btn-combo {
    display: flex;
